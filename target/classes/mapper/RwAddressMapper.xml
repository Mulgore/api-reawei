<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.reawei.api.mapper.RwAddressMapper">

    <!-- type为实体类Student，包名已经配置，可以直接写类名 -->
    <resultMap id="BaseMap" type="cn.reawei.api.model.RwAddress">
        <id property="id" column="id" />
        <result property="memberId" column="member_id" />
        <result property="mobile" column="mobile" />
        <result property="address" column="address" />
        <result property="postcode" column="postcode" />
        <result property="description" column="description" />
        <result property="createTime" column="create_time" />
        <result property="modifyTime" column="modify_time" />
    </resultMap>

    <select id="getAddressList" resultMap="BaseMap">
        SELECT
            id,member_id,mobile,address,postcode,description,create_time,modify_time
         FROM
          rw_address
        WHERE
          1=1
    </select>

</mapper>
